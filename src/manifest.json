{
    "manifest_version": 3,
    "name": "Contest Viewer",
    "description": "View contests from various platforms and other details too.",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "storage",
        "https://*/"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    }
}